{
    "manifest_version": 2,

    "name": "Email Stenography",
    "description": "Has a list of copypastas that you can copy into your clipboard.",
    "version": "1.0.0",

    "content_scripts": [ {
        "matches": ["http://*/*"],
        "js": ["jquery.min.js", "clipboard.min.js", "internet_souschef.js", "popup.js"]
    } ],

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": []
}
